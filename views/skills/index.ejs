<%- include('../partials/header') %>

<ul>
  <% skills.forEach(function(skill) { %>
    <li>
      <h3><%= skill.skill %></h3>
      <p><strong>Description:</strong> <%= skill.description %></p>
      <p><strong>Experience:</strong> <%= skill.experience %></p>
    </li>
  <% }); %>
</ul>

